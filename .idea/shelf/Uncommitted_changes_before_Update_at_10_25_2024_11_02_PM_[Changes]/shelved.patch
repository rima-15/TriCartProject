r\">\r\n\r\n        <div class=\"cartListContainer\">\r\n\r\n            <div class=\"cartProduct\">\r\n                <div class=\"pic-and-info-in-cart\">\r\n                    <img class=\"product-img\" src=\"products-images/matebook-d-16 (2).png\" alt=\"computer\" width=\"100px\" height=\"100px\">\r\n                    <div class=\"cartProductde-details\">\r\n                        <h2 class=\"infoOfProduct\"> HUAWEI MateBook D16</h2>\r\n                        <p class=\"infoOfProductDisc\"> 16\" Intel Core i5 8 GB RAM 512 GB Light Gray </p>\r\n                        <p class=\"category-product-price\">2999.00 SR</p>\r\n                    </div>\r\n                </div>\r\n                <br>\r\n                <hr style=\"border:0.5px solid #567C8D;\">\r\n                <div class=\"PruductControle\" >\r\n                    <button class=\"remove\"> <i class=\"fas fa-trash\"></i></button>\r\n                    <input type=\"number\" name=\"quantity\" value=\"1\" min=\"0\" max=\"10\" class=\"quantity\">\r\n                    <!-- <input type=\"text\" name=\"quantity\" value=\"1\" minlength=\"0\" maxlength=\"10\" size=\"2\">-->\r\n                    <button class=\"add\"> <i class=\"fas fa-plus\"></i></button>\r\n                </div>\r\n            </div>\r\n\r\n            <div class=\"cartProduct\">\r\n                <div class=\"pic-and-info-in-cart\">\r\n                    <img class=\"product-img\" src=\"products-images/Galaxy-S24-fe.png\" alt=\"computer\" width=\"100px\" height=\"100px\">\r\n                        <div class=\"cartProductde-details\">\r\n                            <h2 class=\"infoOfProduct\">Galaxy S24 FE</h2>\r\n                            <p class=\"infoOfProductDisc\">256 GB 8 GB RAM Graphite 5G Exynos 2400 </p>\r\n                            <p class=\"category-product-price\">2849.00 SR</p>\r\n                        </div>\r\n                </div>\r\n                <br>\r\n                <hr style=\"border:0.5px 