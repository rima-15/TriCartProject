<+>\r\n\r\n/* Style for breadcrumb */\r\n    .breadcrumb {\r\n        list-style: none; /* Remove bullet points */\r\n        padding: 0; /* Remove default padding */\r\n        margin: 5px 1px; /* Add some margin */\r\n        text-indent:2%;\r\n    }\r\n\r\n    .breadcrumb li {\r\n        font-family: Georgia, serif;\r\n        display: inline-block; /* Use inline-block for horizontal layout */\r\n        margin-right: 5px; /* Space between items */\r\n        color: #68A8CA;\r\n    }\r\n\r\n    .breadcrumb a {\r\n        text-decoration: none; /* Remove underline from links */\r\n        color: green ;/* Change link color */\r\n    }\r\n\r\n    .breadcrumb a:hover {\r\n        text-decoration: none; /* Underline on hover */\r\n        color: #9e9756;\r\n    }\r\n\r\n    .breadcrumb .separator {\r\n        color: #9E9756; /* Color for the separator */\r\n    }\r\n/*style for Container product */\r\n.products-container{\r\ndisplay: flex;\r\nflex-wrap: wrap;\r\nmargin:0 auto;\r\ngap:20px;\r\npadding:20px;\r\njustify-content:center;\r\n\r\n}\r\n\r\n/*style for one product */\r\n.product {\r\nbackground-color:#FCF8DB;\r\nborder: 2px solid #9E9756;\r\n    padding: 5px;\r\n    margin : 15px;\r\n    width: 250px;\r\n    height: 550px;\r\n    text-align: left;\r\n    border-radius: 8px;\r\n    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n    transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n}\r\n\r\n\r\n/* Hover effect on each product */\r\n.product:hover {\r\n    transform: translateY(-5px);\r\n    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n.product img {\r\nalign:center;\r\nwidth: 99%;\r\nheight:70%;\r\n}\r\n\r\n\r\n.product button:hover{\r\n transform: translateY(-5px);\r\n    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.2);\r\n}\r\n\r\n\r\nh1.categoryName{\r\ncolor :#68A8CA;\r\ntext-indent:4%;\r\n\r\n}\r\n\r\np.CategoryDiscr{\r\ncolor:#9e9756;\r\ntext-indent:5%;\r\n\r\n}\r\n\r\n\r\n.sort{\r\nfont-size:18px;\r\ntext-align: left;\r\nmargin:20px;\r\ncolor:#68A8CA;\r\ntext-indent :3%;\r\n}\r\nselect{\r\npadding: 2px;\r\nfont-size:15px;\r\nborder-radius:5px;\r\nborder :1px solid #68A8CA ;\r\ncolor:#68A8CA;\r\n\r\n\r\n}\r\noptgroup  option{\r\nfont-weight: bold;\r\ncolor : #9e9756;\r\n}\r\n\r\n\r\nbutton.addToCart{\r\nwidth:80px;\r\nheight:80px;\r\ndisplay: flex;\r\njustify-content:center;\r\nalign-items=center;\r\nbackground-color:#FCF8DB;\r\nborder : none;\r\npadding: 10px 20px;\r\ncursor :pointer;\r\nborder-radius:20px;\r\n\r\n}\r\nbutton.addToCart img{\r\nwidth:100%;\r\nheight:100%;\r\n}\r\n.p-and-b{\r\ndisplay:flex;\r\njustify-content:space-between;\r\nalign-items:center;\r\n\r\n\r\n}\r\n\r\n\r\n.p-and-b strong{\r\ncolor :#68A8CA;\r\n}\r\n.infoOfProduct{\r\nfont-size:20px;\r\nweight:bolder;\r\ncolor:green;\r\n}\r\n.infoOfProductDisc{\r\noverflow: hidden;\r\n}\r\n/*Cart page */\r\nh1.CartName{\r\ncolor :#68A8CA;\r\ntext-indent:4%;\r\n\r\n}\r\n\r\np.CartDiscr{\r\ncolor:#9e9756;\r\ntext-indent:5%;\r\n\r\n}\r\n\r\n* {\r\n    box-sizing: border-box;\r\n    margin: 0;\r\n    padding: 0;\r\n}\r\n\r\n\r\n\r\n.cart-items-summary-Container {\r\n    display: grid;\r\n    grid-template-columns: 2fr 1fr;\r\n    gap: 20px;\r\n}\r\n\r\n\r\nh1 {\r\n    font-size: 24px;\r\n    margin: 20px 0;\r\n}\r\n\r\n.cartListContainer {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 20px;\r\n}\r\n\r\n.cartProduct {\r\n    display: flex;\r\n    border: 1px solid #ccc;\r\n    padding: 20px;\r\n    background-color: #fff;\r\n}\r\n\r\n.product-img {\r\n\r\n    background-color: white;\r\n    margin-right: 20px;\r\n    float:left;\r\n}\r\n\r\n.cartProductde-details {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n}\r\n\r\n.priceOftheProduct {\r\n    font-weight: bold;\r\n    margin: 10px 0;\r\n}\r\n\r\n.PruductControle {\r\n    display: flex;\r\n    align-items: center;\r\n}\r\n\r\n.PruductControle button,\r\n.PruductControle input {\r\n    padding: 5px;\r\n    margin: 0 5px;\r\n}\r\n\r\n.orderSummary {\r\n    background-color: #fff;\r\n    border: 1px solid #ccc;\r\n    padding: 20px;\r\n}\r\n\r\n.orderSummary h2 {\r\n    font-size: 20px;\r\n    margin-bottom: 10px;\r\n}\r\n\r\n.orderSummary p {\r\n    margin: 10px 0;\r\n    display: flex;\r\n    justify-content: space-between;\r\n}\r\n\r\nbutton {\r\n    padding: 10px;\r\n    margin-top: 20px;\r\n    cursor: pointer;\r\n}\r\n\r\n.empty {\r\n    background-color: #ffcccc;\r\n    border: 1px solid red;\r\n    color: red;\r\n}\r\n\r\n.checkout {\r\n    background-color: #4CAF50;\r\n    color: white;\r\n    border: none;\r\n}\r\n.PruductControle .remove i {\r\ncolor : blue;\r\n}\r\n
